    "jspdf": "2.5.1",
    "jspdf-autotable": "3.5.28",
    "piexif": "0.0.2",
    "piexifjs": "1.0.6",
    "pinia": "2.0.33",
    "pinia-plugin-persistedstate": "3.1.0",
    "pinia-shared-state": "^0.3.0",
    "primeflex": "3.3.0",
    "primeicons": "6.0.1",
    "quill": "1.3.7",
