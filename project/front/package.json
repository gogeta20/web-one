{
  "name": "xorfor3",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "scripts": {
    "local": "vite --host --mode preview",
    "dev": "vite --host --mode development",
    "stg": "vite --host --mode staging",
    "pro": "vite --host --mode production",
    "extranet": "vite --host --mode extranet",
    "host": "vite --host",
    "build": "vue-tsc --noEmit && vite build --mode production",
    "build:dev": "vue-tsc --noEmit && vite build --mode development",
    "build:stg": "vue-tsc --noEmit && vite build --mode staging",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint src/ --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "coverage": "vitest run --coverage --environment jsdom"
  },
